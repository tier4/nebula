<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>nebula_ros_velodyne</name>
  <version>0.3.0</version>
  <description>Nebula Velodyne ROS Wrapper</description>
  <maintainer email="contact@map4.jp">MAP IV</maintainer>

  <license>Apache 2</license>
  <author>TIER IV</author>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>
  <buildtool_depend>ros_environment</buildtool_depend>
  <buildtool_depend>ros_testing</buildtool_depend>
  <build_depend>libpcl-all-dev</build_depend>
