<?xml version="1.0"?>
<launch>
    <arg name="sensor_model" description="TutorialSensor | TutorialSensor2 | ..."/>
    <arg name="config_file" default="$(find-pkg-share nebula_ros)/config/lidar/tutorial/$(var sensor_model).param.yaml"/>


    <node pkg="nebula_ros" exec="tutorial_ros_wrapper_node"
        name="nebula_tutorial" output="screen">
        <param from="$(var config_file)" allow_substs="true"/>
    </node>
</launch>
